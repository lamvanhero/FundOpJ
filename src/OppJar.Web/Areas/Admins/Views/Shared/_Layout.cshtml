<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>OppJar</title>
    <meta name="author" content="" />
    <meta name="description" content="Crowdfunding Savings Accounts for Every Child" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimal-ui" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- for ios 7 style, multi-resolution icon of 152x152 -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-barstyle" content="black-translucent">
    <link rel="apple-touch-icon" href="/assets/images/logo.png">
    <meta name="apple-mobile-web-app-title" content="Zeus">
    <!-- for Chrome on Android, multi-resolution icon of 196x196 -->
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="shortcut icon" sizes="196x196" href="/favicon.ico">

    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="~/lib/toastr.js/toastr.min.css" type="text/css" />
    <link rel="stylesheet" href="~/lib/datatables/css/jquery.dataTables.min.css" type="text/css" />
    <link rel="stylesheet" href="~/css/app.css" type="text/css" />
    <link rel="stylesheet" href="~/css/font.css" type="text/css" />
    <link rel="stylesheet" href="~/css/site.css" type="text/css" />

    <script src="~/lib/jquery/jquery.min.js" type="text/javascript"></script>

    <style>
        .admin-nav a {
            color: #ccc !important;
        }
    </style>
</head>
<body style=" margin:0 auto">
    @await Html.PartialAsync("_MainHeader")

    <div class="app center">
        <div class="app-content box-shadow-z0 tansparent" role="main">
            <div style="background-color: rgb(78, 117, 151); height: 56px; margin-top: 56px;">
                <div id="admin-nav" class="p-t" style="display: flex; justify-content: space-around; padding-top:22px ;">
                    <a style="color:white" href="/Admins"><h6>Admin Users</h6></a>
                </div>
            </div>

            @RenderBody()
        </div>
    </div>

    <script src="~/js/site.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/lib/tether/js/tether.min.js" type="text/javascript"></script>
    <script src="~/lib/bootstrap/bootstrap.js" type="text/javascript"></script>
    <script src="~/lib/toastr.js/toastr.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery-cookie/jquery.cookie.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
